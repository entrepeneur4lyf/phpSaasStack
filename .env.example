# Application
APP_ENV=development
APP_DEBUG=true
APP_KEY=base64:your-base64-encoded-app-key-here
APP_URL=http://localhost

# Session
SESSION_ENCRYPTION_KEY=your-very-secure-session-encryption-key-here

# Database
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=your_database_name
DB_USERNAME=your_database_username
DB_PASSWORD=your_database_password

# Swoole
SWOOLE_HOST=0.0.0.0
SWOOLE_PORT=443
SWOOLE_SSL_CERT=/path/to/ssl/certificate.crt
SWOOLE_SSL_KEY=/path/to/ssl/private.key

# JWT
JWT_SECRET=your-jwt-secret-here
JWT_EXPIRATION=3600

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
PIAPI_API_KEY=your_piapi_api_key_here

# CORS
CORS_ALLOWED_ORIGINS=https://example.com,https://www.example.com

# Logging
LOG_CHANNEL=stack
LOG_LEVEL=debug

# Mail
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

# Redis (if you decide to use it for caching or queues)
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Cache
CACHE_DRIVER=redis
CACHE_PREFIX=myapp_cache: