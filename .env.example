# Application
APP_NAME=YourAppName
APP_ENV=development
APP_DEBUG=true
ROLLBAR_ACCESS_TOKEN=5d509a541589467c94cf110312e6f142
APP_KEY=base64:your-base64-encoded-app-key-here
APP_URL=http://localhost
APP_PORT=9501
# Add other sensitive configuration values here
ERROR_REPORTING_SERVICE=local

# Session
SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_COOKIE_NAME=your_app_session
SESSION_ENCRYPTION_KEY=your-very-secure-session-encryption-key-here

# Database
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=your_database
DB_USERNAME=your_username
DB_PASSWORD=your_password

# Redis
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=null
REDIS_DATABASE=0

# Swoole
SWOOLE_HTTP_HOST=0.0.0.0
SWOOLE_HTTP_PORT=9501
SWOOLE_WORKER_NUM=4
SWOOLE_TASK_WORKER_NUM=4

# JWT
JWT_SECRET=your_jwt_secret_key_here
JWT_TTL=3600

# File Storage
STORAGE_DRIVER=local
STORAGE_LOCAL_PATH=/path/to/your/storage

# External Services
STRIPE_KEY=your_stripe_key
STRIPE_SECRET=your_stripe_secret

# CORS
CORS_ALLOWED_ORIGINS=https://example.com,https://www.example.com

# Logging
LOG_CHANNEL=stack
LOG_LEVEL=debug

# Mail
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

# Queue
QUEUE_CONNECTION=redis

# Websocket
WEBSOCKET_ENABLED=true
WEBSOCKET_PORT=9502

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=60
RATE_LIMIT_PER_MINUTES=1